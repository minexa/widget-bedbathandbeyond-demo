.tvp-watch-more-text {
  display: inline-block;
  position: relative;
  top: -10px;
  left: -1px;
  font-size: 10px;
}

.tvp-progress-bar {
  background-color: #B82927 !important;
}

#tvplb.off {
  visibility: hidden;
  opacity: 0;
  pointer-events: none;
}
#tvplb .lb-overlay {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  transition: opacity .35s ease-in-out;
  background-color: rgba(0, 0, 0, 0.45);
  position: fixed;
  z-index: 9999;
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  outline: 0;
  visibility: visible;
  opacity: 1;
  pointer-events: auto;
  display: none;
}
#tvplb .lb-content {
  transition: opacity .25s ease-in-out;
  background-color: #FFF;
