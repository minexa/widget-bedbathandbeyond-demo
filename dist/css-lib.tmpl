/*
    Aspect Ratio
*/
#tvp-gallery {
  transition: opacity .25s ease-in-out;
  opacity: 0.6;
}
#tvp-gallery.enabled {
  opacity: 1;
}

#tvpchg-slider-header {
  margin-left: 43px;
}
#tvpchg-slider-header .header-link {
  font-size: 14px;
  line-height: 1.36;
  font-family: "Open Sans", Helvetica, Arial, sans-serif;
  font-weight: 400;
  margin-left: 10px;
}
#tvpchg-slider-header .header-link span:first-child {
  margin-right: 6px;
  text-decoration: underline;
}
#tvpchg-slider-header .header-link i:first-child {
  display: inline-block;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: inherit;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
}
#tvpchg-slider-header .slick-dots {
  text-align: left;
  height: 33px;
  line-height: 0px;
  padding-left: 4px;
}
#tvpchg-slider-header .slick-dots > li {
  display: inline-block;
  margin-right: 5px;
}
#tvpchg-slider-header .slick-dots > li button {
  cursor: pointer;
  display: block;
  width: 6px;
  height: 6px;
  padding: 0;
  margin: 0;
  border-radius: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  background-color: #000000;
  border: 1px solid #000000;
  opacity: 1;
  font-size: 0;
  color: transparent;
}
#tvpchg-slider-header .slick-dots > li button:hover {
  background-color: #ffc010;
  opacity: 1;
}
#tvpchg-slider-header .slick-dots > li.slick-active > button {
  background-color: transparent;
  opacity: 1;
}

.tvpchg-slider-headline {
  font: 400 28px 'Open Sans Condensed', 'Helvetica Narrow', arial, helvetica, sans-serif;
  text-transform: uppercase;
  letter-spacing: 1px;
  vertical-align: baseline;
}

#tvpchg-slider {
  position: relative;
  width: 95.85%;
  margin: 0 auto;
  /* Slider */
}
#tvpchg-slider * {
  outline: none;
}
#tvpchg-slider .slick-slider {
  position: relative;
  display: block;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-touch-callout: none;
  -khtml-user-select: none;
  -ms-touch-action: pan-y;
  touch-action: pan-y;
  -webkit-tap-highlight-color: transparent;
}
#tvpchg-slider .slick-list {
  position: relative;
  display: block;
  overflow: hidden;
  margin: 0;
  padding: 0;
  margin-left: 18px;
  margin-right: 18px;
}
#tvpchg-slider .slick-list:focus {
  outline: none;
}
#tvpchg-slider .slick-list.dragging {
  cursor: pointer;
  cursor: hand;
  display: none;
}
#tvpchg-slider .slick-slider .slick-track,
#tvpchg-slider .slick-slider .slick-list {
  -webkit-transform: translate3d(0, 0, 0);
  -moz-transform: translate3d(0, 0, 0);
  -ms-transform: translate3d(0, 0, 0);
  -o-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}
#tvpchg-slider .slick-track {
  position: relative;
  top: 0;
  left: 0;
  display: block;
}
#tvpchg-slider .slick-arrow {
  font-size: 0;
  color: transparent;
  background-color: transparent;
  border: 0;
  position: absolute;
  top: -50px;
  bottom: 0;
  margin: auto;
  width: 30px;
  height: 30px;
  z-index: 999;
}
#tvpchg-slider .slick-track:before,
#tvpchg-slider .slick-track:after {
  display: table;
  content: '';
}
#tvpchg-slider .slick-track:after {
  clear: both;
}
#tvpchg-slider .slick-loading .slick-track {
  visibility: hidden;
}
#tvpchg-slider .slick-slide {
  float: left;
  height: 100%;
  min-height: 1px;
}
#tvpchg-slider [dir='rtl'] .slick-slide {
  float: right;
}
#tvpchg-slider .slick-slide img {
  display: block;
}
#tvpchg-slider .slick-slide.slick-loading img {
  display: none;
}
#tvpchg-slider .slick-slide.dragging img {
  pointer-events: none;
}
#tvpchg-slider .slick-initialized .slick-slide {
  display: block;
}
#tvpchg-slider .slick-loading .slick-slide {
  visibility: hidden;
}
#tvpchg-slider .slick-vertical .slick-slide {
  display: block;
  height: auto;
  border: 1px solid transparent;
}
#tvpchg-slider .slick-arrow.slick-hidden {
  display: none;
}

.tvp-arrow-prev {
  cursor: pointer;
  transition: all .25s ease-in-out;
  position: relative;
  top: 40px;
  font-size: 40px;
  float: left;
}
.tvp-arrow-prev:hover {
  color: orange;
}

.tvp-arrow-next {
  cursor: pointer;
  transition: all .25s ease-in-out;
  top: 40px;
  position: relative;
  font-size: 40px;
  float: right;
}
.tvp-arrow-next:hover {
  color: orange;
}

#tvpchg-slider .tvp-video {
  display: block;
  margin: 0 5px;
  text-decoration: none;
  color: #000;
  cursor: pointer;
  outline: none;
}
@media screen and (max-width: 767px) {
  #tvpchg-slider .tvp-video:first-child {
    margin-left: 0px;
  }
}
#tvpchg-slider .tvp-video.active .video-overlay {
  opacity: 1;
}
#tvpchg-slider .tvp-video.active .video-playing {
  opacity: 1;
}
#tvpchg-slider .tvp-video.active .video-play-button {
  display: none;
}
#tvpchg-slider .tvp-video:not(.active):hover .video-play-button,
#tvpchg-slider .tvp-video:not(.active):hover .play-now,
#tvpchg-slider .tvp-video:not(.active):hover .video-overlay {
  opacity: 1;
}
#tvpchg-slider .tvp-video:not(.active):hover .tvp-video-play-button {
  background-color: #ed145b;
}
#tvpchg-slider .tvp-video:focus {
  outline: none;
}
#tvpchg-slider .tvp-video p {
  margin: 0;
}
#tvpchg-slider .tvp-video .tittle {
  display: block;
  margin-top: 5px;
  font-size: 13px;
  text-align: left;
  line-height: 18px;
  font-family: lato, sans-serif;
  overflow: hidden;
}
#tvpchg-slider .tvp-video .tittle:hover {
  color: #0000ff;
}
#tvpchg-slider .tvp-video-image {
  position: relative;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  position: relative;
  pointer-events: none;
}
#tvpchg-slider .tvp-video-image:before {
  display: block;
  content: "";
  width: 100%;
  padding-top: 56.25%;
}
#tvpchg-slider .tvp-video-image > .ar-content {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
#tvpchg-slider .tvp-video-image .video-overlay {
  -moz-transition: all 0.25s ease;
  -o-transition: all 0.25s ease;
  -webkit-transition: all 0.25s ease;
  transition: all 0.25s ease;
  pointer-events: none;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  opacity: 0;
  -webkit-transition: opacity 60ms ease-out;
  transition: opacity 60ms ease-out;
}
#tvpchg-slider .tvp-video-image .video-playing {
  text-align: center;
  color: #fff;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  height: 25px;
  margin: auto;
  font-weight: bold;
  line-height: 25px;
  opacity: 0;
  font-size: 1.2em;
}
#tvpchg-slider .tvp-video-image .play-now {
  text-align: center;
  color: #fff;
  position: absolute;
  bottom: 0;
  right: 0;
  height: 25px;
  margin: auto;
  font-weight: bold;
  line-height: 25px;
  opacity: 0;
  font-size: 1.2em;
}
#tvpchg-slider .tvp-video-play-button {
  cursor: pointer;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 7px;
  right: 0;
  margin: auto;
  z-index: 9999;
  background-repeat: no-repeat;
  background-color: rgba(237, 20, 91, 0.71);
  width: 40px;
  height: 40px;
}

[class^='tvp'],
[class*=' tvp'] {
  box-sizing: border-box;
}

[id^='tvp_html5_video_'] {
  padding: 0 !important;
}

.tvp-cr-embed {
  position: relative;
}

.tvp-cr-player {
  background-color: black;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  overflow: hidden;
  padding: 0 !important;
}

.tvp-cr-player.queued {
  overflow: visible;
}

.tvp-cr-player > div {
  padding: 0 !important;
}

.tvp-cr-big-play {
  cursor: pointer;
  background-color: #2b2b2b;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 9999;
}

.tvp-cr-big-play:hover {
  background-color: #F9A21B;
  background: linear-gradient(to bottom, #f78c1b, #FEB13A);
}

.tvp-cr-big-play:after {
  content: " ";
  width: 0;
  height: 0;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.tvp-cr-big-play.small {
  width: 100px;
  height: 50px;
  border-radius: 2px;
}

.tvp-cr-big-play.small:after {
  border-top: 9px solid transparent;
  border-bottom: 9px solid transparent;
  border-left: 18px solid #fff;
}

.tvp-cr-player-overlay {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.tvp-cr-player-overlay.small .tvp-cr-player-overlay-icon {
  background-color: black;
  display: none;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.tvp-cr-player-overlay-icon:after {
  content: " ";
  width: 0;
  height: 0;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.small .tvp-cr-player-overlay-icon {
  width: 45px;
  height: 45px;
  border-radius: 50%;
}

.small.play .tvp-cr-player-overlay-icon:after {
  border-top: 7px solid transparent;
  border-bottom: 7px solid transparent;
  border-left: 13px solid #fff;
}

.small.pause .tvp-cr-player-overlay-icon:after {
  width: 3px;
  height: 14px;
  border-right: 4px solid #fff;
  border-left: 4px solid #fff;
}

.tvp-cr-magno-bar {
  background-color: rgba(0, 0, 0, 0.7);
  position: absolute;
  bottom: 0;
  width: 100%;
  zoom: 1;
}

.tvp-cr-play {
  cursor: pointer;
  z-index: 99;
  float: left;
  position: relative;
}

.tvp-cr-play.small {
  width: 35px;
  height: 45px;
}

.tvp-cr-play.play:after {
  content: " ";
  width: 0;
  height: 0;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.tvp-cr-play.play.small:after {
  border-top: 7px solid transparent;
  border-bottom: 7px solid transparent;
  border-left: 13px solid #fff;
}

.tvp-cr-play.pause:after {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 0;
  bottom: 0;
}

.tvp-cr-play.pause.small:after {
  width: 3px;
  height: 14px;
  border-right: 4px solid #fff;
  border-left: 4px solid #fff;
}

.tvp-cr-play-bar {
  z-index: 99;
  float: right;
  position: relative;
}

.tvp-cr-play-bar.small {
  height: 45px;
}

.tvp-cr-play-bar .tvp-cr-time {
  position: absolute;
  left: 0;
}

.tvp-cr-play-bar .tvp-cr-time > span {
  display: inline-block;
  color: white !important;
}

.tvp-cr-play-bar .tvp-cr-time.small {
  width: 81px;
  height: 100%;
  line-height: 46px;
  font-size: 12px;
  color: white;
  text-align: left;
  padding: 0 !important;
}

.tvp-cr-play-bar #tvp-cr-mute {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
  cursor: pointer;
  width: 30px;
  height: 30px;
  background-image: url(data:image/png;base64,);
  background-position: -30px -60px;
  background-repeat: no-repeat;
  opacity: 0.8;
  margin: 0 5px 0 8px;
}

.tvp-cr-play-bar #tvp-cr-mute:hover {
  opacity: 1;
}

.tvp-cr-play-bar #tvp-cr-mute.muted {
  background-position: -30px -90px;
  opacity: 0.8;
}

.tvp-cr-play-bar #tvp-cr-mute.muted:hover {
  opacity: 1;
}

.tvp-cr-play-bar .tvp-cr-volume {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto 0;
  padding-top: 0 !important;
  padding-bottom: 0 !important;
}

.tvp-cr-play-bar .tvp-cr-volume > div {
  background-color: #565656;
  width: 4px;
  height: 100%;
  float: left;
  margin-left: 2px;
  cursor: pointer;
  padding: 0 !important;
}

.tvp-cr-play-bar .tvp-cr-volume > div.on {
  background-color: white;
}

.tvp-cr-play-bar .tvp-cr-volume > div:first-child {
  margin-left: 0;
}

.tvp-cr-play-bar .tvp-cr-volume.small {
  width: 49px;
  height: 17px;
  padding: 0;
}

.tvp-cr-play-bar .tvp-cr-full-button {
  position: absolute;
  right: 0;
}

.tvp-cr-play-bar .tvp-cr-full-button.small {
  width: 40px;
  height: 100%;
  cursor: pointer;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 22px 22px;
  background-position: center center;
}

.tvp-cr-play-bar .tvp-cr-prog-bar {
  cursor: pointer;
  border: 1px solid #565656;
  position: absolute;
  bottom: 0;
  top: 0;
  margin: auto;
  padding: 0 !important;
}

.tvp-cr-play-bar .tvp-cr-prog-bar > div {
  background-color: white;
  width: 0%;
  height: 100%;
  position: absolute;
  left: 0;
  padding: 0 !important;
}

.tvp-cr-play-bar .tvp-cr-prog-bar.small {
  height: 10px;
}

.tvp-cr-video-poster {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 100%;
  background-repeat: no-repeat;
  background-size: cover;
}

